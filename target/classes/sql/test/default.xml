<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">  
<!--
	각 파일의 namespace가 중복 되지 않도록 설정
-->
<mapper namespace="SQL.DEF">	

	<select id="SELECT_SQL" resultType="Map">
		${SQL_A}
  	</select>  	  	  	
  	
  	<update id="UPDATE_SQL">
		${SQL_A}
  	</update>  	  	  	
  	
	<insert id="INSERT_SQL">
		${SQL_A}
  	</insert>  	

	<delete id = 'DELETE_SQL'>
		${SQL_A}
	</delete>
  	  	  	 	
</mapper>